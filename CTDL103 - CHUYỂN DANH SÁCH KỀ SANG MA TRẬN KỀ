#include<bits/stdc++.h>
using namespace std;
		int n;
		int a[1000][1000];
int main(){
		cin>>n;
		cin.ignore();
		for(int i=1;i<=n;i++){
			string s,num;
			getline(cin,s);//su dung cai nay thay vi cin>>s vÃ¬ cin>>s se ket thuc khi nhap dau cach hoac xuong dong
			stringstream ss(s);//tach tung phan tu cua s luu vao ss
			while(ss>>num){//gan tung phan tu cua ss vao num
				a[i][stoi(num)]=1;
			}
		}
		for(int i=1;i<=n;i++){
			for(int j=1;j<=n;j++){
				cout<<a[i][j]<<" ";
			}
			cout<<endl;
		}
	
}
