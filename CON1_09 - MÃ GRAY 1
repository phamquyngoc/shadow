#include<bits/stdc++.h>
using namespace std;
int n;
string a[10000];
int main(){
	int t;
	cin>>t;
	while(t--){
		cin>>n;
		int l=pow(2,n);
		a[1]="0";
		a[2]="1";
		if(n>1){
			int vt=2;//vi tri lay doi xung
			for(int i=2;i<=n;i++){
				int k=pow(2,i);
				for(int j=1;j<=vt;j++){
					a[k-j+1]="1"+a[j];
					a[j]="0"+a[j];
				}
				vt=k;
			}
		}for(int i=1;i<=l;i++){
			cout<<a[i]<<" ";
		}cout<<endl;
	}
}
