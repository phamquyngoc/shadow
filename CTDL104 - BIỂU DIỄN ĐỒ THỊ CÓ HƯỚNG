#include<bits/stdc++.h>
using namespace std;
int main(){
	int t;
	cin>>t;
	while(t--){
		int n,m;
vector<int> a[1000];
		cin>>n>>m;
		for(int i=0;i<m;i++){
			int x,y;
			cin>>x>>y;
			a[x].push_back(y);
		}
		for(int i=1;i<=n;i++){
			cout<<i<<": ";
			for(int x:a[i]){
				cout<<x<<" ";
			}cout<<endl;
		}
	}
}
